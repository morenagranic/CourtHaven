<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="84dp"
    android:orientation="vertical">


    <TextView
        android:id="@+id/history"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:fontFamily="@font/open_sans_bold"
        android:gravity="center"
        android:text="History"
        android:textSize="16sp" />

    <View
        android:id="@+id/SplitLine_history"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="-6dp"
        android:layout_marginBottom="6dp"
        android:layout_marginHorizontal="8dp"
        android:background="@color/purple" />

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="@color/lighter_gray"
        app:cardCornerRadius="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                android:id="@+id/pic3"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="8dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/basketball_court6"
                    android:contentDescription="basketball court" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/court3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/pic3"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic3"
                android:fontFamily="@font/open_sans_semi_bold"
                android:text="Court 6"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/owner3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/court3"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic3"
                android:fontFamily="@font/open_sans_light"
                android:text="Address 6"
                android:textSize="12sp" />

            <View
                android:id="@+id/SplitLine_hor3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/owner3"
                android:layout_margin="8dp"
                android:layout_toEndOf="@+id/pic3"
                android:background="@color/light_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/SplitLine_hor3"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic3"
                android:fontFamily="@font/open_sans_regular"
                android:text="Wednesday, 13 December:\n11:30 (90 min)" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="@color/lighter_gray"
        app:cardCornerRadius="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                android:id="@+id/pic4"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="8dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/basketball_court12"
                    android:contentDescription="basketball court" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/court4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/pic4"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic4"
                android:fontFamily="@font/open_sans_semi_bold"
                android:text="Court 12"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/owner4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/court4"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic4"
                android:fontFamily="@font/open_sans_light"
                android:text="Address 12"
                android:textSize="12sp" />

            <View
                android:id="@+id/SplitLine_hor4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/owner4"
                android:layout_margin="8dp"
                android:layout_toEndOf="@+id/pic4"
                android:background="@color/light_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/SplitLine_hor4"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic4"
                android:fontFamily="@font/open_sans_regular"
                android:text="Thursday, 13 April\n19:30 (60 min)" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="@color/lighter_gray"
        app:cardCornerRadius="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                android:id="@+id/pic5"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="8dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/football_court9"
                    android:contentDescription="basketball court" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/court5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/pic5"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic5"
                android:fontFamily="@font/open_sans_semi_bold"
                android:text="Court 9"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/owner5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/court5"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic5"
                android:fontFamily="@font/open_sans_light"
                android:text="Address 9"
                android:textSize="12sp" />

            <View
                android:id="@+id/SplitLine_hor5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/owner5"
                android:layout_margin="8dp"
                android:layout_toEndOf="@+id/pic5"
                android:background="@color/light_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/SplitLine_hor5"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic5"
                android:fontFamily="@font/open_sans_regular"
                android:text="Saturday, 28 August:\n 11:30 (120 min)" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="10dp"
        android:backgroundTint="@color/lighter_gray"
        app:cardCornerRadius="8dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.cardview.widget.CardView
                android:id="@+id/pic6"
                android:layout_width="150dp"
                android:layout_height="100dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                app:cardCornerRadius="8dp">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/basketball_court12"
                    android:contentDescription="basketball court" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/court6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/pic6"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic6"
                android:fontFamily="@font/open_sans_semi_bold"
                android:text="Court 12"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/owner6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/court6"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic6"
                android:fontFamily="@font/open_sans_light"
                android:text="Address 12"
                android:textSize="12sp" />

            <View
                android:id="@+id/SplitLine_hor6"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/owner6"
                android:layout_margin="8dp"
                android:layout_toEndOf="@+id/pic6"
                android:background="@color/light_gray" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/SplitLine_hor6"
                android:layout_marginStart="8dp"
                android:layout_toEndOf="@+id/pic6"
                android:fontFamily="@font/open_sans_regular"
                android:text="Friday, 8 September:\n21:00 (60 min)" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>



</LinearLayout>